def  sum(a,b)   :
    c=a+b

x=sum(4,5)
print(x)
def even_odd(num):
    if num%2==0:
        x=1
    else:
        x=0
    return x
numm=int(11)
res=even_odd(numm)
if res==1:
    print("even")
else:
    print("odd")

def fact(n):
    pro =1
    while(n>=1):
        pro=pro*n
        n=n-1
        return pro

for i in range(1,10):
    print("factorial",fact(i))
